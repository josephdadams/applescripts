<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>DriveSpaceCheck</key>
    <string>com.technician.drivespacecheck</string>
    <key>KeepAlive</key>
    <false/>
    <key>RunAtLoad</key>
    <false/>
    <key>ProgramArguments</key>
		<array>
			<string>/usr/bin/osascript</string>
			<string>/Users/technician/Documents/Scripts/drivespacecheck.scpt</string>
		</array>
	<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>7</integer>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
</dict>
</plist>